# QuantMind 生产环境配置文件
# 请根据实际情况修改以下配置

# 域名配置
DOMAIN=quantmind.example.com
PROTOCOL=https

# SSL配置
SSL_TYPE=letsencrypt
SSL_EMAIL=admin@example.com
NGINX_PORT=80
NGINX_SSL_PORT=443

# MySQL配置
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_DATABASE=quantmind
MYSQL_USER=quantmind
MYSQL_PASSWORD=your_secure_user_password_here
MYSQL_HOST=mysql
MYSQL_PORT=3306

# Redis配置
REDIS_PASSWORD=your_secure_redis_password_here
REDIS_HOST=redis
REDIS_PORT=6379

# InfluxDB配置
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=your_secure_influxdb_password_here
INFLUXDB_ORG=quantmind
INFLUXDB_BUCKET=market_data
INFLUXDB_HOST=influxdb
INFLUXDB_PORT=8086

# JWT配置
JWT_SECRET=your_secure_jwt_secret_here

# API密钥 (可选)
JUHE_API_KEY=
QWEN_API_KEY=
GEMINI_API_KEY=

# 环境设置
ENVIRONMENT=production
DEBUG=false

# 日志配置
LOG_LEVEL=info